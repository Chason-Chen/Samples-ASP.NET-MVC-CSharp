@using Stimulsoft.Report.Mvc;

@{
    ViewBag.Title = "Stimulsoft Reports.Web Demo";
}

@Html.Stimulsoft().StiMvcDesigner("StiMvcDesigner1", new StiMvcDesignerOptions()
{
    Actions =
    {
        GetReport = "GetReport",
        SaveReport = "SaveReport",
        DesignerEvent = "DesignerEvent",
        Exit = "ExitDesigner"
    },
    FileMenu =
    {
        ShowExit = true
    }
})